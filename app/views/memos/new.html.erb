<div class="mb-3">    
    <h1>New page</h1>
</div>

<%= form_with url: create_path do |form| %>
    <div class="mb-3">
        <%= form.label :Title, for: "exampleFormControlInput1", class: "form-label" %>
        <%= form.text_field :title, class: "form-control", id: "exampleFormControlInput1", placeholder: "Title", autocomplete: "off" %>
    </div>
    <div class="mb-3">
        <%= form.label :Content, for: "exampleFormControlInput2", class: "form-label" %>
        <%= form.text_field :body, class: "form-control", id: "exampleFormControlInput2", placeholder: "Content", autocomplete: "off" %>
    </div>
    <div class="mb-3">
        <%= form.submit :submit, class: "btn btn-primary" %>
    </div>
<% end %>